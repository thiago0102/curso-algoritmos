algoritmo "exercicio 3 "

var
   nome: caractere
  salario: real
  
inicio

       escreval(" nome do funcionario ")
       leia ( nome)
       escreval ("digite seu salario ....")
       leia (salario)
       
       escreval ("O funcionario " , nome , " tem um salario de " , salario ," em junho  desse mes "  )


fimalgoritmo
